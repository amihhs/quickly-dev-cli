{
  "name": "quickly-dev",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@7.5.1",
  "description": "Quickly build a local development environment",
  "author": {
    "name": "amihhs",
    "email": "amihhs369@gmail.com",
    "url": "https://github.com/amihhs"
  },
  "scripts": {
    "build": "rimraf packages/*/dist && esno scripts/copy-files.ts && pnpm -r --filter=./packages/* run build && pnpm -r run build-post",
    "release": "bumpp -r",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/types": "^17.4.4",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.14.2",
    "bumpp": "^9.0.0",
    "eslint": "^8.33.0",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "make-empty-dir": "^2.0.0",
    "pkg": "^5.8.0",
    "typescript": "^4.9.3",
    "unbuild": "^1.1.2",
    "vitest": "^0.29.2"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md}": "eslint --fix"
  }
}